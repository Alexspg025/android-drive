<resources>
    <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
    <plurals name="trash_operation_successful_format">
        <item quantity="one">1 item sent to trash</item>
        <item quantity="other">%1$d items sent to trash</item>
    </plurals>
    <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
    <plurals name="trash_restore_operation_successful_format">
        <item quantity="one">1 item restored</item>
        <item quantity="other">%d items restored</item>
    </plurals>
    <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
    <plurals name="trash_delete_operation_successful_format">
        <item quantity="one">1 item deleted permanently</item>
        <item quantity="other">%d items deleted permanently</item>
    </plurals>
    <string name="trash_empty_operation_successful">Trash emptied</string>
    <string name="trash_error_occurred_sending_to_trash">An error occurred while trying to send your file(s) to the trash</string>
    <string name="trash_error_occurred_restoring_from_trash">An error occurred while trying to restore your file(s)</string>
    <string name="trash_error_occurred_deleting_from_trash">An error occurred while trying to delete the file(s)</string>
    <string name="trash_error_occurred_emptying_trash">An error occurred while trying to empty the trash</string>
</resources>