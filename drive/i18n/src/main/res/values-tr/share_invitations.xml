<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">%1$s paylaş</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 kişi eklendi</item>
    <item quantity="other">%1$d kişi eklendi</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 kişi davet edilemedi</item>
    <item quantity="other">%1$d kişi davet edilemedi</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">1 kişiyle paylaşılıyor</item>
    <item quantity="other">%1$d kişiyle paylaşılıyor</item>
  </plurals>
  <string name="common_permission_owner">Sahibi</string>
  <string name="common_permission_admin">Yönetici</string>
  <string name="common_permission_editor">Düzenleyici</string>
  <string name="common_permission_viewer">Görüntüleyici</string>
  <string name="common_permission_unknown">Bilinmiyor</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (Bekliyor)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Onaylanmış)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Bilinmiyor)</string>
  <string name="share_via_invitations_add_people_hint">Kişi ekle</string>
  <string name="share_via_invitations_copy_invite_link_action">Davet bağlantısını kopyala</string>
  <string name="share_via_invitations_remove_access_action">Erişimi kaldır</string>
  <string name="share_via_invitations_stop_sharing_description">Bağlantıyı sil ve herkesin erişimini kaldır</string>
  <string name="share_via_invitations_resend_invite_action">Daveti yeniden gönder</string>
  <string name="share_via_invitations_resend_invite_success">Davet yeniden gönderildi</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d üye</item>
    <item quantity="other">%1$d üye</item>
  </plurals>
</resources>
