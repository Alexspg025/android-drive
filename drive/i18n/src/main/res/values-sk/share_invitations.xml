<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Zdieľať %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 osoba pridaná</item>
    <item quantity="few">%1$d osoby pridané</item>
    <item quantity="many">%1$d osôb pridaných</item>
    <item quantity="other">%1$d osôb pridaných</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 osoba nemôže byť pozvaná</item>
    <item quantity="few">%1$d osoby nemôžu byť pozvané</item>
    <item quantity="many">%1$d osôb nemôže byť pozvaných</item>
    <item quantity="other">%1$d osôb nemôže byť pozvaných</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Zdieľanie s 1 osobou</item>
    <item quantity="few">Zdieľanie s %1$d osobami</item>
    <item quantity="many">Zdieľanie s %1$d osobami</item>
    <item quantity="other">Zdieľanie s %1$d osobami</item>
  </plurals>
  <string name="common_permission_owner">Owner</string>
  <string name="common_permission_admin">Admin</string>
  <string name="common_permission_editor">Editor</string>
  <string name="common_permission_viewer">Viewer</string>
  <string name="common_permission_unknown">Unknown</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (Pending)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Accepted)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Unknown)</string>
  <string name="share_via_invitations_add_people_hint">Pridať ľudí</string>
  <string name="share_via_invitations_copy_invite_link_action">Skopírovať pozývací odkaz</string>
  <string name="share_via_invitations_remove_access_action">Odobrať prístup</string>
  <string name="share_via_invitations_stop_sharing_description">Odstrániť odkaz a zrušiť pre všetkých prístup</string>
  <string name="share_via_invitations_resend_invite_action">Znova odoslať pozvánku</string>
  <string name="share_via_invitations_resend_invite_success">Pozvánka bola opätovne odoslaná</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d člen</item>
    <item quantity="few">%1$d členovia</item>
    <item quantity="many">%1$d členov</item>
    <item quantity="other">%1$d členov</item>
  </plurals>
</resources>
