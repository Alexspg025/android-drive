<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">%1$s-ის გაზიარება</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 პიროვნება დამატებულია</item>
    <item quantity="other">%1$d დამატებულია</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 პიროვნების მოწვევა შეუძლებელია</item>
    <item quantity="other">%1$d პიროვნების მოწვევა შეუძლებელია</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">უზიარებთ 1 პირს</item>
    <item quantity="other">უზიარებთ %1$d პირს</item>
  </plurals>
  <string name="share_via_invitations_permission_owner">მფლობელი</string>
  <string name="share_via_invitations_permission_admin">ადმინისტრატორი</string>
  <string name="share_via_invitations_permission_editor">რედაქტორი</string>
  <string name="share_via_invitations_permission_viewer">მთვალიერებელი</string>
  <string name="share_via_invitations_permission_unknown">უცნობი</string>
  <!--
        Variable %1$s is the permission
        Examples:
            "Editor (Invitation sent)"
    -->
  <string name="share_via_invitations_invitation_sent">%1$s (მოსაწვევი გაგზავნილია)</string>
  <string name="share_via_invitations_add_people_hint">ხალხის დამატება</string>
  <string name="share_via_invitations_copy_invite_link_action">მოსაწვევის ბმულის კოპირება</string>
  <string name="share_via_invitations_remove_access_action">წვდომის გაუქმება</string>
  <string name="share_via_invitations_stop_sharing_description">ბმულის წაშლა და წვდომების წაშლა ყველასთვის</string>
  <string name="share_via_invitations_resend_invite_action">მოსაწვევის თავიდან გაგზავნა</string>
  <string name="share_via_invitations_resend_invite_success">მოსაწვევი გადაიგზავნა</string>
</resources>
