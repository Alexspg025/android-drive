<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">%1$s 공유</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="other">%1$d명 추가됨</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="other">%1$d명의 사용자를 초대할 수 없습니다</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="other">%1$d명과 공유</item>
  </plurals>
  <string name="share_via_invitations_permission_owner">소유자</string>
  <string name="share_via_invitations_permission_admin">관리자</string>
  <string name="share_via_invitations_permission_editor">편집자</string>
  <string name="share_via_invitations_permission_viewer">뷰어</string>
  <string name="share_via_invitations_permission_unknown">알 수 없음</string>
  <!--
        Variable %1$s is the permission
        Examples:
            "Editor (Invitation sent)"
    -->
  <string name="share_via_invitations_invitation_sent">%1$s (초대 전송됨)</string>
  <string name="share_via_invitations_add_people_hint">사용자 추가하기</string>
  <string name="share_via_invitations_copy_invite_link_action">초대 링크 복사</string>
  <string name="share_via_invitations_remove_access_action">액세스 제거</string>
  <string name="share_via_invitations_stop_sharing_description">링크를 삭제하고 모두의 접근 권한을 삭제합니다</string>
  <string name="share_via_invitations_resend_invite_action">초대 다시 보내기</string>
  <string name="share_via_invitations_resend_invite_success">초대를 다시 보냈습니다</string>
</resources>
