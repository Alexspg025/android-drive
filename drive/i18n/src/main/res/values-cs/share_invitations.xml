<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Sdílet %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">Přidána 1 osoba</item>
    <item quantity="few">Přidány %1$d osoby</item>
    <item quantity="many">Přidáno %1$d osob</item>
    <item quantity="other">Přidáno %1$d osob</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 osoba nemůže být pozvána</item>
    <item quantity="few">%1$d osoby nemohou být pozvány</item>
    <item quantity="many">%1$d osob nemůže být pozváno</item>
    <item quantity="other">%1$d osob nemůže být pozváno</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Sdílení s 1 osobou</item>
    <item quantity="few">Sdílení se %1$d osobami</item>
    <item quantity="many">Sdílení s %1$d osobami</item>
    <item quantity="other">Sdílení s %1$d osobami</item>
