  <string name="share_via_invitations_remove_access_action">Odebrat přístup</string>
  <string name="share_via_invitations_stop_sharing_description">Smazat odkaz a odebrat všem přístup</string>
  <string name="share_via_invitations_resend_invite_action">Znovu poslat pozvánku</string>
