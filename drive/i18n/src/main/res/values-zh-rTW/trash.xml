<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="other">%1$d 個項目已移動至垃圾桶</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="other">%d 個項目已還原</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="other">%d 個項目已永久刪除</item>
  </plurals>
  <string name="trash_empty_operation_successful">已清空垃圾桶</string>
  <string name="trash_error_occurred_sending_to_trash">嘗試將檔案移動至垃圾桶時發生錯誤</string>
  <string name="trash_error_occurred_restoring_from_trash">嘗試還原檔案時發生錯誤</string>
  <string name="trash_error_occurred_deleting_from_trash">嘗試刪除檔案時發生錯誤</string>
  <string name="trash_error_occurred_emptying_trash">嘗試清空垃圾桶時發生錯誤</string>
</resources>
