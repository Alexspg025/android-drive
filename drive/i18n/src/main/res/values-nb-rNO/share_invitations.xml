<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Del %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 person lagt til</item>
    <item quantity="other">%1$d personer lagt til</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 person kan ikke inviteres</item>
    <item quantity="other">%1$d personer kan ikke inviteres</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Deler med 1 person</item>
    <item quantity="other">Deler med %1$d personer</item>
  </plurals>
  <string name="share_via_invitations_permission_owner">Eier</string>
  <string name="share_via_invitations_permission_admin">Administrator</string>
  <string name="share_via_invitations_permission_editor">Redaktør</string>
  <string name="share_via_invitations_permission_viewer">Leser</string>
  <string name="share_via_invitations_permission_unknown">Ukjent</string>
  <!--
        Variable %1$s is the permission
        Examples:
            "Editor (Invitation sent)"
    -->
  <string name="share_via_invitations_invitation_sent">%1$s (invitasjon sendt)</string>
  <string name="share_via_invitations_add_people_hint">Legg til personer</string>
  <string name="share_via_invitations_copy_invite_link_action">Kopier invitasjonskobling</string>
  <string name="share_via_invitations_remove_access_action">Fjern tilgang</string>
  <string name="share_via_invitations_stop_sharing_description">Slett kobling og fjern tilgangen for alle</string>
  <string name="share_via_invitations_resend_invite_action">Send invitasjon på nytt</string>
  <string name="share_via_invitations_resend_invite_success">Invitasjonen ble sendt på nytt</string>
</resources>
