<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Udostępnij %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">Dodano 1 użytkownika</item>
    <item quantity="few">Dodano %1$d użytkowników</item>
    <item quantity="many">Dodano %1$d użytkowników</item>
    <item quantity="other">Dodano %1$d użytkowników</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 użytkownik nie może zostać zaproszony</item>
    <item quantity="few">%1$d użytkowników nie może zostać zaproszonych</item>
