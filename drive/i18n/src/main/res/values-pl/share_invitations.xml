    <item quantity="other">%1$d użytkowników nie może zostać zaproszonych</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Udostępniono 1 użytkownikowi</item>
    <item quantity="few">Udostępniono %1$d użytkownikom</item>
    <item quantity="many">Udostępniono %1$d użytkownikom</item>
