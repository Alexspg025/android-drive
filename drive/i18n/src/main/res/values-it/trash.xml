<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 elemento spostato nel cestino</item>
    <item quantity="other">%1$d elementi spostati nel cestino</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 elemento ripristinato</item>
    <item quantity="other">%d elementi ripristinati</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 elemento eliminato definitivamente</item>
    <item quantity="other">%d elementi eliminati definitivamente</item>
  </plurals>
  <string name="trash_empty_operation_successful">Cestino svuotato</string>
  <string name="trash_error_occurred_sending_to_trash">Errore nello spostamento di uno o più file nel cestino</string>
  <string name="trash_error_occurred_restoring_from_trash">Errore nel ripristino di uno o più file</string>
  <string name="trash_error_occurred_deleting_from_trash">Errore nell\'eliminazione di uno o più file</string>
  <string name="trash_error_occurred_emptying_trash">Errore nello svuotamento del cestino</string>
