<resources>
    <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
    <string name="title_share_via_invitations">Share %1$s</string>
    <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
    <plurals name="share_via_invitations_person_added">
        <item quantity="one">1 person added</item>
        <item quantity="other">%1$d persons added</item>
    </plurals>
    <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
    <plurals name="share_via_invitations_person_added_error">
        <item quantity="one">1 person cannot be invited</item>
        <item quantity="other">%1$d persons cannot be invited</item>
    </plurals>
    <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
    <plurals name="share_via_invitations_sharing_with">
        <item quantity="one">Sharing with 1 person</item>
        <item quantity="other">Sharing with %1$d persons</item>
    </plurals>

    <string name="share_via_invitations_permission_owner">Owner</string>
    <string name="share_via_invitations_permission_admin">Admin</string>
    <string name="share_via_invitations_permission_editor">Editor</string>
    <string name="share_via_invitations_permission_viewer">Viewer</string>
    <string name="share_via_invitations_permission_unknown">Unknown</string>
    <!--
        Variable %1$s is the permission
        Examples:
            "Editor (Invitation sent)"
    -->
    <string name="share_via_invitations_invitation_sent">%1$s (Invitation sent)</string>
    <string name="share_via_invitations_add_people_hint">Add people</string>
    <string name="share_via_invitations_copy_invite_link_action">Copy invite link</string>
    <string name="share_via_invitations_remove_access_action">Remove access</string>
    <string name="share_via_invitations_stop_sharing_title">@string/common_stop_sharing_action</string>
    <string name="share_via_invitations_stop_sharing_description">Delete link and remove access for everyone</string>
    <string name="share_via_invitations_resend_invite_action">Resend invite</string>
    <string name="share_via_invitations_resend_invite_success">Invitation was resent</string>
</resources>
