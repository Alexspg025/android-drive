<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Jaa %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 henkilö lisättiin</item>
    <item quantity="other">%1$d henkilöä lisättiin</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">Yhtä henkilöä ei voida kutsua</item>
    <item quantity="other">%1$d henkilöä ei voida kutsua</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Jaetaan yhdelle henkilölle</item>
    <item quantity="other">Jaetaan %1$d henkilölle</item>
  </plurals>
  <string name="share_via_invitations_permission_owner">Omistaja</string>
  <string name="share_via_invitations_permission_admin">Ylläpito</string>
  <string name="share_via_invitations_permission_editor">Muokkaus</string>
  <string name="share_via_invitations_permission_viewer">Tarkastelu</string>
  <string name="share_via_invitations_permission_unknown">Tuntematon</string>
  <!--
        Variable %1$s is the permission
        Examples:
            "Editor (Invitation sent)"
    -->
  <string name="share_via_invitations_invitation_sent">%1$s (kutsu lähetettiin)</string>
  <string name="share_via_invitations_add_people_hint">Lisää henkilölle</string>
  <string name="share_via_invitations_copy_invite_link_action">Kopioi kutsulinkki</string>
  <string name="share_via_invitations_remove_access_action">Mitätöi käyttöoikeus</string>
  <string name="share_via_invitations_stop_sharing_description">Poista linkki ja käyttöoikeus kaikilta</string>
  <string name="share_via_invitations_resend_invite_action">Lähetä kutsu uudelleen</string>
  <string name="share_via_invitations_resend_invite_success">Kutsu lähetettiin uudelleen</string>
</resources>
