<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">%1$s teilen</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 Person hinzugefügt</item>
    <item quantity="other">%1$d Personen hinzugefügt</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 Person kann nicht eingeladen werden</item>
    <item quantity="other">%1$d Personen können nicht eingeladen werden</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Geteilt mit 1 Person</item>
    <item quantity="other">Geteilt mit %1$d Personen</item>
  </plurals>
  <string name="common_permission_owner">Besitzer</string>
  <string name="common_permission_admin">Administrator</string>
  <string name="common_permission_editor">Bearbeiter</string>
  <string name="common_permission_viewer">Leseberechtigter</string>
  <string name="common_permission_unknown">Unbekannt</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (Ausstehend)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Angenommen)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Unbekannt)</string>
  <string name="share_via_invitations_add_people_hint">Personen hinzufügen</string>
  <string name="share_via_invitations_copy_invite_link_action">Einladungslink kopieren</string>
  <string name="share_via_invitations_remove_access_action">Zugriff entfernen</string>
  <string name="share_via_invitations_stop_sharing_description">Link löschen und Zugriff für alle entfernen</string>
  <string name="share_via_invitations_resend_invite_action">Einladung erneut senden</string>
  <string name="share_via_invitations_resend_invite_success">Einladung wurde erneut gesendet</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d Mitglied</item>
    <item quantity="other">%1$d Mitglieder</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Add a message</string>
  <string name="share_via_invitations_send_message_and_name_label">Include message and file name in invite email</string>
</resources>
