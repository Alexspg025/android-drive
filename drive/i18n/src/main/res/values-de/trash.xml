<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 Element in den Papierkorb verschoben</item>
    <item quantity="other">%1$d Elemente in den Papierkorb verschoben</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 Element wiederhergestellt</item>
    <item quantity="other">%d Elemente wiederhergestellt</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 Element dauerhaft gelöscht</item>
    <item quantity="other">%d Elemente dauerhaft gelöscht</item>
  </plurals>
  <string name="trash_empty_operation_successful">Papierkorb geleert</string>
  <string name="trash_error_occurred_sending_to_trash">Beim Verschieben deiner Datei(en) in den Papierkorb ist ein Fehler aufgetreten.</string>
  <string name="trash_error_occurred_restoring_from_trash">Beim Wiederherstellen deiner Datei(en) ist ein Fehler aufgetreten.</string>
  <string name="trash_error_occurred_deleting_from_trash">Beim Löschen deiner Datei(en) ist ein Fehler aufgetreten.</string>
  <string name="trash_error_occurred_emptying_trash">Beim Versuch, den Papierkorb zu leeren, ist ein Fehler aufgetreten.</string>
</resources>
